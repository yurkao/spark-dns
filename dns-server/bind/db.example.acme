$TTL    86400
@       IN      SOA     localhost. root.localhost. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                          86400 )       ; Negative Cache TTL
;
@       IN      NS      localhost.
workstation1      IN      A       192.168.3.2
workstation2      IN      A       192.168.5.2
dns-server      IN      A       192.168.3.3
